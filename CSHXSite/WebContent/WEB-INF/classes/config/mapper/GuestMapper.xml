<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easyway.website.cshx.mapper.GuestMapper">
	<insert id="insert">
		insert into cms_guestmsg
		(clientip,date,content,mail,phone,status) values
		(#{clientip},now(),#{content},#{mail},#{phone},5)
	</insert>
</mapper>